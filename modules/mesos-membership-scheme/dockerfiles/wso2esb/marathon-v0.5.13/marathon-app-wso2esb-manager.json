{
    "id": "/product/service/wso2esb-manager",
    "cpus": 0.5,
    "mem": 800.0,
    "requirePorts": false,
    "instances": 1,
    "executor": "",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "wso2esb-manager:4.9.0",
            "network": "BRIDGE",
            "portMappings": [
               { "containerPort": 0, "hostPort": 0, "protocol": "tcp" },
               { "containerPort": 9763, "hostPort": 0, "protocol": "tcp" },
               { "containerPort": 9443, "hostPort": 0, "protocol": "tcp" },
               { "containerPort": 8280, "hostPort": 0, "protocol": "tcp" },
               { "containerPort": 8243, "hostPort": 0, "protocol": "tcp" }
            ],
            "privileged": false,
            "parameters": [
             
            ]
        },
        "volumes": [
            {
                "containerPath": "/mnt/mesos-artifacts/esb",
                "hostPath": "/mnt/mesos-artifacts/esb",
                "mode": "RW"
            }
        ]
    },
    "env": {
        "WSO2_SERVER_PROFILE": "manager"
    },
    "constraints": [
       
    ],
    "labels": {
        "environment": "dev"
    },
    "fetch": [
       
    ],
    "dependencies": [],
    "healthChecks": [
        
    ],
    "backoffSeconds": 1,
    "backoffFactor": 1.15,
    "maxLaunchDelaySeconds": 3600,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0.5,
        "maximumOverCapacity": 0.2
    }
}
