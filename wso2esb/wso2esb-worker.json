{
  "id": "wso2esb-worker",
  "cpus": 0.2,
  "mem": 700,
  "disk": 0,
  "instances": 1,
  "container": {
    "docker": {
      "image": "wso2esb-worker:4.9.0",
      "network": "BRIDGE",
      "portMappings": [
        {
          "name": "hazelcast",
          "containerPort": 0,
          "hostPort": 0,
          "protocol": "tcp"
        },
        {
          "containerPort": 8280,
          "servicePort": 10091,
          "protocol": "tcp",
          "name": "pass-through-http"
        },
        {
          "containerPort": 8243,
          "servicePort": 10092,
          "protocol": "tcp",
          "name": "pass-through-https"
        }
      ]
    },
    "type": "DOCKER"
  },
  "env": {
    "KEY_STORE_PASSWORD": "wso2carbon"
  },
  "labels": {
    "HAPROXY_GROUP": "marathon-lb"
  }
}
