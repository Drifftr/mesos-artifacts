{
  "id": "wso2esb-default",
  "cpus": 0.2,
  "mem": 700,
  "instances": 1,
  "executor": "",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "wso2esb:4.9.0",
      "network": "BRIDGE",
      "portMappings": [
        {
          "name": "hazelcast",
          "containerPort": 0,
          "hostPort": 0,
          "protocol": "tcp"
        },
        {
          "containerPort": 8280,
          "servicePort": 10091,
          "protocol": "tcp",
          "name": "pass-through-http"
        },
        {
          "containerPort": 8243,
          "servicePort": 10092,
          "protocol": "tcp",
          "name": "pass-through-https"
        },
        {
          "containerPort": 9763,
          "servicePort": 10093,
          "protocol": "tcp",
          "name": "servlet-http"
        },
        {
          "containerPort": 9443,
          "servicePort": 10094,
          "protocol": "tcp",
          "name": "servlet-https"
        }
      ]
    },
    "volumes": [
      {
        "containerPath": "/mnt/mesos-artifacts/esb",
        "hostPath": "/mnt/mesos-artifacts/esb",
        "mode": "RW"
      }
    ]
  },
  "env": {
    "KEY_STORE_PASSWORD": "wso2carbon"
  },
  "constraints": [
  ],
  "labels": {
    "environment": "dev",
    "HAPROXY_GROUP": "marathon-lb"
  },
  "fetch": [
  ],
  "dependencies": [],
  "healthChecks": [
  ],
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "upgradeStrategy": {
    "minimumHealthCapacity": 0.5,
    "maximumOverCapacity": 0.2
  }
}
