{
  "id": "wso2esb-default",
  "cpus": 0.2,
  "mem": 700,
  "instances": 1,
  "executor": "",
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "wso2esb:4.9.0",
      "network": "BRIDGE",
      "portMappings": [
        {
          "name": "hazelcast",
          "containerPort": 0,
          "hostPort": 0,
          "protocol": "tcp"
        },
        {
          "containerPort": 8280,
          "servicePort": 10091,
          "protocol": "tcp",
          "name": "pass-through-http"
        },
        {
          "containerPort": 8243,
          "servicePort": 10092,
          "protocol": "tcp",
          "name": "pass-through-https"
        },
        {
          "containerPort": 9763,
          "servicePort": 10093,
          "protocol": "tcp",
          "name": "servlet-http"
        },
        {
          "containerPort": 9443,
          "servicePort": 10094,
          "protocol": "tcp",
          "name": "servlet-https"
        }
      ]
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/",
      "portIndex": 1
    }
  ],
  "env": {
    "KEY_STORE_PASSWORD": "wso2carbon"
  },
  "labels": {
    "HAPROXY_GROUP": "marathon-lb"
  }
}
