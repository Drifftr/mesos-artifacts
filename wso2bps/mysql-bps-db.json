{
  "id": "mysql-bps-db",
  "cmd": null,
  "cpus": 0.25,
  "mem": 128,
  "disk": 0,
  "instances": 1,
  "container": {
    "docker": {
      "image": "mysql:5.5",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 3306,
          "hostPort": 10031,
          "protocol": "tcp",
          "name": "server-port"
        }
      ]
    },
    "type": "DOCKER"
  },
  "env": {
    "MYSQL_ROOT_PASSWORD": "root",
    "MYSQL_DATABASE": "BPS_DB",
    "MYSQL_USER": "wso2carbon",
    "MYSQL_PASSWORD": "wso2carbon"
  },
  "healthChecks": [
    {
      "protocol": "TCP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3
    }
  ]
}
