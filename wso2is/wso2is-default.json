{
    "id": "wso2is-default",
    "cpus": 0.5,
    "mem": 800.0,
    "instances": 1,
    "executor": "",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "wso2is:5.1.0",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 9763,
                    "servicePort": 10111,
                    "protocol": "tcp",
                    "name": "servlet-http",
                    "labels": {
                        "VIP_0": "9763"
                    }
                },
                {
                    "containerPort": 9443,
                    "servicePort": 10112,
                    "protocol": "tcp",
                    "name": "servlet-https",
                    "labels": {
                        "VIP_1": "9443"
                    }
                },
                {
                    "containerPort": 8000,
                    "servicePort": 10113,
                    "protocol": "tcp",
                    "name": "kdc-server",
                    "labels": {
                        "VIP_2": "8000"
                    }
                },
                {
                    "containerPort": 10500,
                    "servicePort": 10114,
                    "protocol": "tcp",
                    "name": "thrift-entitlement",
                    "labels": {
                        "VIP_3": "10500"
                    }
                }
            ]
        },
        "volumes": [
            {
                "containerPath": "/mnt/mesos-artifacts/is",
                "hostPath": "/mnt/mesos-artifacts/is",
                "mode": "RW"
            }
        ]
    },
    "env": {
        "KEY_STORE_PASSWORD": "wso2carbon"
    },
    "constraints": [

    ],
    "labels": {
        "environment": "dev",
        "HAPROXY_GROUP": "marathon-lb"
    },
    "fetch": [

    ],
    "dependencies": [],
    "healthChecks": [

    ],
    "backoffSeconds": 1,
    "backoffFactor": 1.15,
    "maxLaunchDelaySeconds": 3600,
    "upgradeStrategy": {
        "minimumHealthCapacity": 0.5,
        "maximumOverCapacity": 0.2
    }
}
