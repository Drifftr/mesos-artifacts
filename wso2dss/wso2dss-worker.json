{
    "id": "wso2dss-worker",
    "cpus": 0.5,
    "mem": 1024,
    "disk": 0,
    "instances": 1,
    "container": {
        "docker": {
            "image": "wso2dss-worker:3.5.0",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 9763,
                    "servicePort": 10073,
                    "protocol": "tcp",
                    "name": "servlet-http",
                    "labels": {
                        "VIP_0": "9763"
                    }
                },
                {
                    "containerPort": 9443,
                    "servicePort": 10074,
                    "protocol": "tcp",
                    "name": "servlet-https",
                    "labels": {
                        "VIP_1": "9443"
                    }
                }
            ]
        },
        "type": "DOCKER"
    },
    "env": {
        "KEY_STORE_PASSWORD": "wso2carbon"
    },
    "labels": {
        "HAPROXY_GROUP": "marathon-lb"
    }
}
